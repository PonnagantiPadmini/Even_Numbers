/* Series of even between 2 numbers*/
import java.util.*;
class even
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int a,b;
		System.out.print("Enter the values");
		a=sc.nextInt();
		b=sc.nextInt();
		while(a<=b)
		{
		if(a%2==0)
		System.out.print(a+"  ");
               a++;
		}
	}
}
